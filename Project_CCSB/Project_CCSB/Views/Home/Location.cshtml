<style type="text/css">
    
    #map {
        height: 400px;
        width: 100%;
    }
</style>
<script>
    // Initialize and add the map
    function initMap() {
        // The location of storage
        const storage = { lat: 52.225, lng: 6.951 };
        // The map, centered at storage
        const map = new google.maps.Map(document.getElementById("map"), {
            zoom: 16,
            center: storage,
        });
        // The marker, positioned at storage
        const marker = new google.maps.Marker({
          position: storage,
          map: map,
        });
      }
</script>

<body>
    <div id="map"></div>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBseIGHQWVIkoR8sqF_Ie5TnM3g87LU01g&callback=initMap&libraries=&v=weekly&channel=2"
            async></script>
</body>